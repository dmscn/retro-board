{
  "name": "retro-board",
  "version": "0.0.1",
  "description": "Retrospective Board Web Application",
  "homepage": "https://dmscn.github.io/retro-board/",
  "main": "App.js",
  "scripts": {
    "dev": "webpack-dev-server --open --watch --config config/webpack/dev.js",
    "build": "webpack --config config/webpack/prod.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "pretty-quick --pattern '**/*.*(js|jsx)' --verbose",
    "lint": "eslint \"**/*.{js,jsx}\" --quiet",
    "lint:fix": "npm run lint -- --fix ",
    "predeploy": "npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --quiet --fix",
      "pretty-quick --verbose"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/dmscn/retro-board.git"
  },
  "keywords": [
    "retro",
    "scrum",
    "web",
    "react",
    "app",
    "pwa"
  ],
  "author": "Leonardo Damasceno",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dmscn/retro-board/issues"
  },
  "dependencies": {
    "@gympass/yoga": "^7.20.2",
    "@gympass/yoga-icons": "^1.3.2",
    "@react-pdf/renderer": "^2.0.21",
    "dotenv": "^8.2.0",
    "firebase": "^7.14.1",
    "react": "^16.12.0",
    "react-animations": "^1.0.0",
    "react-dom": "^16.12.0",
    "react-feather": "^2.0.8",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "styled-components": "^5.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@svgr/webpack": "^5.3.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.2",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.1",
    "lint-staged": "^10.2.11",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  }
}
